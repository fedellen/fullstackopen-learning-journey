query get {
  allBooks {
    title
    id
    published
    genres
    author {
      name
      bookCount
      born
      id
    }
  }
}

query count {
  authorCount
  bookCount
}

mutation setBorn {
  setBirthYear(name: "Derek Thompson", born: 2001) {
    name
    born
    bookCount
    id
  }
}

